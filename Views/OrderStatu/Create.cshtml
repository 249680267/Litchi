@model LitchiServer.Models.OR_OrderStatu

@{
    ViewBag.Title = "Create";
}

<h2>新建订单状态</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>OR_OrderStatu</legend>

       

        <div class="editor-label">
            状态名称：
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.nvrStatusName)
            @Html.ValidationMessageFor(model => model.nvrStatusName)
        </div>

        <div class="editor-label">
          状态描述：
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.nvrStatusDescription)
            @Html.ValidationMessageFor(model => model.nvrStatusDescription)
        </div>

       

        <p>
            <input type="submit" value="确认" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("返回列表", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
